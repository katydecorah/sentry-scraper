{
  "name": "Sentry scraper",
  "description": "Takes a Sentry Issue to make a GitHub issue",
  "version": "1.2",
  "background": {
    "scripts": ["jquery.js", "background.js", "content.js"]
  },
  "permissions": ["tabs", "http://*/*", "https://*/*"],
  "browser_action": {
    "default_title": "Sentry scraper"
  },
  "icons": {
    "128": "icon128.png"
  },
  "manifest_version": 2
}
